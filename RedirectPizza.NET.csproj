<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageProjectUrl>https://github.com/LJFloor/RedirectPizza.NET</PackageProjectUrl>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <RepositoryUrl>https://github.com/LJFloor/RedirectPizza.NET</RepositoryUrl>
        <PackageTags>RedirectPizza.NET redirect.pizza</PackageTags>
        <PackageReadmeFile>README.md</PackageReadmeFile>
        <Copyright>Â© LJFloor 2022</Copyright>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Macross.Json.Extensions" Version="3.0.0" />
      <PackageReference Include="RestSharp" Version="108.0.3" />
      <None Include="README.md" Pack="true" PackagePath="\" />
    </ItemGroup>

</Project>
